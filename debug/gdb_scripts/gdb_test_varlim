set args <dataCube.rsf varlim=y om0=5 dm0=0.1 nm0=5 ot0=1.1 dt0=0.1 nt0=5 v0=1.5 verb=y repeat=1 rnmaxfile=rnmax.rsf rnminfile=rnmin.rsf rnipmaxfile=rnipmax.rsf rnipminfile=rnipmin.rsf betamaxfile=betamax.rsf betaminfile=betamin.rsf > teste.rsf
